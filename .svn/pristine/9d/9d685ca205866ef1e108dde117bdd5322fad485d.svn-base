package kr.ac.ajou.umc.vo.event;

import io.swagger.annotations.ApiModelProperty;

import java.util.Date;

public class EventSearchListVO {

    @ApiModelProperty(notes = "primary key")
    private long id;

    @ApiModelProperty(notes = "이벤트 관계자 id")
    private long userId;

    @ApiModelProperty(notes = "이벤트 위도")
    private double lat;

    @ApiModelProperty(notes = "이벤트 경도")
    private double lon;

    @ApiModelProperty(notes = "사용자 gps와 이벤트 사이의 거리")
    private double distance;

    @ApiModelProperty(notes = "이벤트 타이틀")
    private String eventTitle;

    @ApiModelProperty(notes = "이벤트 이미지 경로")
    private String imageLink;

    @ApiModelProperty(notes = "이벤트 시작시간")
    private Date startTime;

    @ApiModelProperty(notes = "이벤트 종료시간")
    private Date endTime;

    public long getId() {
        return id;
    }

    public void setId(long id) {
        this.id = id;
    }

    public long getUserId() {
        return userId;
    }

    public void setUserId(long userId) {
        this.userId = userId;
    }

    public double getLat() {
        return lat;
    }

    public void setLat(double lat) {
        this.lat = lat;
    }

    public double getLon() {
        return lon;
    }

    public void setLon(double lon) {
        this.lon = lon;
    }

    public double getDistance() {
        return distance;
    }

    public void setDistance(double distance) {
        this.distance = distance;
    }

    public String getEventTitle() {
        return eventTitle;
    }

    public void setEventTitle(String eventTitle) {
        this.eventTitle = eventTitle;
    }

    public String getImageLink() {
        return imageLink;
    }

    public void setImageLink(String imageLink) {
        this.imageLink = imageLink;
    }

    public Date getStartTime() {
        return startTime;
    }

    public void setStartTime(Date startTime) {
        this.startTime = startTime;
    }

    public Date getEndTime() {
        return endTime;
    }

    public void setEndTime(Date endTime) {
        this.endTime = endTime;
    }
}
